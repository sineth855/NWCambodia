<div class="userRegisterPopUp" id="bodyheadertext_login" style="display: none;">
    <div id="modal_containerPopUp">
        <div id="modal_inside" class="wrapper-popup">
            <div class="model_content" style="padding: 10px;">
                <div class="header-title_">{{text_user_account}}</div>
                 <div class="btn-wrap-user-close">
                    <a href="javascript:void(0);" id="close_" class="js-modal-close continue-btn close-fancy">X</a>
                </div>
                 <div class="mobile-register text-center">
                    {% if logged == null %}
                    <div>
                        <span><a href="{{login}}" class="button promotion-button"><i class="ti-user"></i> &nbsp; {{text_login}}</a></span>
                        <span><a href="{{register}}" class="button promotion-button"><i class="ti-lock"></i> &nbsp; {{text_register}}</a></span>
                    </div>
                    {% else %}
                    <div>
                        <span><a href="#" class="button promotion-button"><i class="ti-user"></i> &nbsp; {{lastname}} {{firstname}}</a></span>
                        <span><a href="{{logout}}" class="button promotion-button"><i class="ti-lock"></i> &nbsp; {{text_logout}}</a></span>
                    </div>
                    {% endif %}
                </div>
            </div>
        </div>
    </div>
</div>


